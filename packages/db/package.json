    "@types/node": "^18.15.11",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.37.0",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
